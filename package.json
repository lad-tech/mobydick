{
  "name": "@lad-tech/mobydick",
  "version": "7.35.0-next.11",
  "private": true,
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "preinstall": "./scripts/env.sh",
    "postinstall": "npx patch-package",
    "postversion": "./scripts/semantic-release.ts",
    "release": "semantic-release",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:ci": "jest --ci --coverage=true",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc -p tsconfig.json --noEmit",
    "lint:ci": "eslint . --quiet && tsc -p tsconfig.json --noEmit",
    "build:development:ios-simulator": "eas build --platform ios --profile development-simulator --local --non-interactive",
    "build:development:ios": "eas build -p ios --local --profile development --non-interactive",
    "build:development:android": "eas build -p android --local --profile development --non-interactive",
    "build:ios": "eas build -p ios --local --non-interactive",
    "build:android": "eas build -p android --local --non-interactive",
    "eas:install:development-simulator": "eas build:run --profile development-simulator",
    "eas:install:development": "eas build:run --profile development",
    "eas:install:production": "eas build:run --profile production",
    "e2e:test": "maestro test ./e2e/main-flow.yaml"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@react-navigation/bottom-tabs": "^6.5.18",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/react-native-skia": "1.5.0",
    "expo": "^52.0.0",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.16",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-calendars": "^1.1294.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@lad-tech/eslint-config": "^0.0.16",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^29.5.13",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.0",
    "@types/xdate": "^0.8.32",
    "@types/yargs": "^17.0.10",
    "babel-jest": "^29.6.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "lefthook": "^1.7.16",
    "prettier": "^3.2.4",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^23.0.0",
    "simple-git": "^3.10.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.3",
    "winston": "^3.8.1"
  },
  "engines": {
    "node": ">=18"
  }
}